sourcesContent":["<template>\r\n  <div class=\"page-W2\">\r\n    <h1>B1-K1-W2 - Technisch en Functioneel Ontwerp</h1>\r\n\r\n    <div class=\"text-block\">\r\n      <h2>Vertaling van user stories naar ontwerp</h2>\r\n      <!-- ... jouw tekst ... -->\r\n    </div>\r\n\r\n    <!-- Loop over sets -->\r\n    <div v-for=\"(set, setIndex) in documentSets\" :key=\"setIndex\" class=\"document-set\">\r\n\r\n      <!-- Per set 3 documenten naast elkaar -->\r\n      <div class=\"documents-row\">\r\n\r\n        <div\r\n          v-for=\"(doc, docIndex) in set\"\r\n          :key=\"docIndex\"\r\n          class=\"document-container\"\r\n        >\r\n          <div class=\"iframes-block\">\r\n            <h3>{{ doc.title }}</h3>\r\n            <iframe\r\n              v-for=\"(src, i) in doc.iframes\"\r\n              :key=\"i\"\r\n              :src=\"src\"\r\n              width=\"100%\"\r\n              height=\"400px\"\r\n              frameborder=\"0\"\r\n            ></iframe>\r\n          </div>\r\n\r\n          <div class=\"side-block\">\r\n            <div class=\"buttons\">\r\n              <button\r\n                v-for=\"(img, i) in doc.images\"\r\n                :key=\"i\"\r\n                @click=\"selectedImages[setIndex * 3 + docIndex] = i\"\r\n                :class=\"{ active: selectedImages[setIndex * 3 + docIndex] === i }\"\r\n              >\r\n                Afbeelding {{ i + 1 }}\r\n              </button>\r\n            </div>\r\n\r\n            <img :src=\"doc.images[selectedImages[setIndex * 3 + docIndex]]\" alt=\"Diagram\" />\r\n          </div>\r\n        </div>\r\n\r\n      </div>\r\n    </div>\r\n\r\n    <br /><br /><br />\r\n\r\n    <!-- Jouw tekstblokken hieronder... -->\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"PageTwo\",\r\n  data() {\r\n    return {\r\n      // 3 sets, elk met 3 documenten\r\n      documentSets: [\r\n        [\r\n          {\r\n            title: \"Document 1-1\",\r\n            iframes: [\"/doc/doc1-1.pdf\"],\r\n            images: [\r\n              \"/images/doc1-1-img1.png\",\r\n              \"/images/doc1-1-img2.png\",\r\n              \"/images/doc1-1-img3.png\",\r\n              \"/images/doc1-1-img4.png\",\r\n            ],\r\n          },\r\n          {\r\n            title: \"Document 1-2\",\r\n            iframes: [\"/doc/doc1-2.pdf\"],\r\n            images: [\r\n              \"/images/doc1-2-img1.png\",\r\n              \"/images/doc1-2-img2.png\",\r\n              \"/images/doc1-2-img3.png\",\r\n              \"/images/doc1-2-img4.png\",\r\n            ],\r\n          },\r\n          {\r\n            title: \"Document 1-3\",\r\n            iframes: [\"/doc/doc1-3.pdf\"],\r\n            images: [\r\n              \"/images/doc1-3-img1.png\",\r\n              \"/images/doc1-3-img2.png\",\r\n              \"/images/doc1-3-img3.png\",\r\n              \"/images/doc1-3-img4.png\",\r\n            ],\r\n          },\r\n        ],\r\n        [\r\n          {\r\n            title: \"Document 2-1\",\r\n            iframes: [\"/doc/doc2-1.pdf\"],\r\n            images: [\r\n              \"/images/doc2-1-img1.png\",\r\n              \"/images/doc2-1-img2.png\",\r\n              \"/images/doc2-1-img3.png\",\r\n              \"/images/doc2-1-img4.png\",\r\n            ],\r\n          },\r\n          {\r\n            title: \"Document 2-2\",\r\n            iframes: [\"/doc/doc2-2.pdf\"],\r\n            images: [\r\n              \"/images/doc2-2-img1.png\",\r\n              \"/images/doc2-2-img2.png\",\r\n              \"/images/doc2-2-img3.png\",\r\n              \"/images/doc2-2-img4.png\",\r\n            ],\r\n          },\r\n          {\r\n            title: \"Document 2-3\",\r\n            iframes: [\"/doc/doc2-3.pdf\"],\r\n            images: [\r\n              \"/images/doc2-3-img1.png\",\r\n              \"/images/doc2-3-img2.png\",\r\n              \"/images/doc2-3-img3.png\",\r\n              \"/images/doc2-3-img4.png\",\r\n            ],\r\n          },\r\n        ],\r\n        [\r\n          {\r\n            title: \"Document 3-1\",\r\n            iframes: [\"/doc/doc3-1.pdf\"],\r\n            images: [\r\n              \"/images/doc3-1-img1.png\",\r\n              \"/images/doc3-1-img2.png\",\r\n              \"/images/doc3-1-img3.png\",\r\n              \"/images/doc3-1-img4.png\",\r\n            ],\r\n          },\r\n          {\r\n            title: \"Document 3-2\",\r\n            iframes: [\"/doc/doc3-2.pdf\"],\r\n            images: [\r\n              \"/images/doc3-2-img1.png\",\r\n              \"/images/doc3-2-img2.png\",\r\n              \"/images/doc3-2-img3.png\",\r\n              \"/images/doc3-2-img4.png\",\r\n            ],\r\n          },\r\n          {\r\n            title: \"Document 3-3\",\r\n            iframes: [\"/doc/doc3-3.pdf\"],\r\n            images: [\r\n              \"/images/doc3-3-img1.png\",\r\n              \"/images/doc3-3-img2.png\",\r\n              \"/images/doc3-3-img3.png\",\r\n              \"/images/doc3-3-img4.png\",\r\n            ],\r\n          },\r\n        ],\r\n      ],\r\n\r\n      // houd per document welke afbeelding actief is (9 totaal)\r\n      selectedImages: Array(9).fill(0),\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.page-W2 {\r\n  color: white;\r\n  padding: 40px;\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  font-family: sans-serif;\r\n}\r\n\r\nh1 {\r\n  margin-bottom: 20px;\r\n  color: #ffcad4;\r\n}\r\n\r\nh2,\r\nh3 {\r\n  margin-top: 20px;\r\n  color: #ffd6e0;\r\n}\r\n\r\n.text-block {\r\n  margin-bottom: 30px;\r\n  line-height: 1.6;\r\n}\r\n\r\n/* Per set: rij met 3 documenten */\r\n.document-set {\r\n  margin-bottom: 60px;\r\n}\r\n\r\n.documents-row {\r\n  display: flex;\r\n  gap: 25px;\r\n  justify-content: space-between;\r\n}\r\n\r\n/* Per document container: iframe links, buttons + afbeelding rechts */\r\n.document-container {\r\n  display: flex;\r\n  flex: 1 1 0;\r\n  gap: 12px;\r\n  background: #222;\r\n  padding: 15px;\r\n  border-radius: 10px;\r\n  min-width: 320px;\r\n}\r\n\r\n/* iframe blok */\r\n.iframes-block {\r\n  flex: 2;\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 12px;\r\n}\r\n\r\n/* rechterkolom: buttons + afbeelding */\r\n.side-block {\r\n  flex: 1;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: flex-end;\r\n  gap: 12px;\r\n}\r\n\r\n/* Buttons grid: 2 per rij */\r\n.buttons {\r\n  display: grid;\r\n  grid-template-columns: repeat(2, 1fr);\r\n  gap: 10px 15px;\r\n  width: 100%;\r\n  max-width: 200px;\r\n}\r\n\r\n.buttons button {\r\n  padding: 8px 14px;\r\n  font-size: 14px;\r\n  background-color: #444;\r\n  border: none;\r\n  color: white;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n  width: 100%;\r\n}\r\n\r\n.buttons button:hover {\r\n  background-color: #666;\r\n}\r\n\r\n.buttons button.active {\r\n  background-color: #ffcad4;\r\n  color: black;\r\n  font-weight: bold;\r\n}\r\n\r\n/* Afbeelding onder buttons */\r\n.side-block img {\r\n  width: 100%;\r\n  border-radius: 8px;\r\n  object-fit: contain;\r\n  max-height: 200px;\r\n  box-shadow: 0 0 12px #aaa;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAS;;EAYXA,KAAK,EAAC;AAAe;;EAOjBA,KAAK,EAAC;AAAe;mBApBpC;;EAgCeA,KAAK,EAAC;AAAY;;EAChBA,KAAK,EAAC;AAAS;mBAjChC;mBAAA;;uBACEC,mBAAA,CAsDM,OAtDNC,UAsDM,G,0BArDJC,mBAAA,CAAoD,YAAhD,6CAA2C,sB,0BAE/CA,mBAAA,CAGM;IAHDH,KAAK,EAAC;EAAY,IACrBG,mBAAA,CAAgD,YAA5C,yCAAuC,GAC3CC,mBAAA,wBAA2B,C,sBAG7BA,mBAAA,oBAAuB,G,kBACvBH,mBAAA,CAuCMI,SAAA,QAjDVC,WAAA,CAUmCC,KAAA,CAAAC,YAAY,EAV/C,CAUiBC,GAAG,EAAEC,QAAQ;yBAA1BT,mBAAA,CAuCM;MAvCwCU,GAAG,EAAED,QAAQ;MAAEV,KAAK,EAAC;QAEjEI,mBAAA,uCAA0C,EAC1CD,mBAAA,CAmCM,OAnCNS,UAmCM,I,kBAjCJX,mBAAA,CA+BMI,SAAA,QA9CdC,WAAA,CAgBoCG,GAAG,EAhBvC,CAgBkBI,GAAG,EAAEC,QAAQ;2BADvBb,mBAAA,CA+BM;QA7BHU,GAAG,EAAEG,QAAQ;QACdd,KAAK,EAAC;UAENG,mBAAA,CAUM,OAVNY,UAUM,GATJZ,mBAAA,CAAwB,YAAAa,gBAAA,CAAjBH,GAAG,CAACI,KAAK,mB,kBAChBhB,mBAAA,CAOUI,SAAA,QA7BtBC,WAA