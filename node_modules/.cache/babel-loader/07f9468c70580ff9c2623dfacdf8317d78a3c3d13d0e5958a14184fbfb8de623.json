s-column\">\r\n      <div\r\n        v-for=\"(doc, index) in documents\"\r\n        :key=\"index\"\r\n      >\r\n        <div class=\"document-container\">\r\n          <div class=\"iframes-block\">\r\n            <h3>{{ doc.title }}</h3>\r\n            <iframe\r\n              v-for=\"(src, i) in doc.iframes\"\r\n              :key=\"i\"\r\n              :src=\"src\"\r\n              width=\"100%\"\r\n              height=\"400px\"\r\n              frameborder=\"0\"\r\n            ></iframe>\r\n          </div>\r\n\r\n          <div class=\"side-block\">\r\n            <div class=\"buttons\">\r\n              <button\r\n                v-for=\"(btn, i) in doc.buttons\"\r\n                :key=\"i\"\r\n                @click=\"() => updateSelectedImage(index, i)\"\r\n                :class=\"{ active: selectedImages[index] === i }\"\r\n              >\r\n                {{ btn }}\r\n              </button>\r\n            </div>\r\n\r\n            <img\r\n              :src=\"doc.images[selectedImages[index]]\"\r\n              alt=\"afbeelding\"\r\n            />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <br /><br /><br />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"EisenEnWensen\",\r\n  data() {\r\n    return {\r\n      documents: [\r\n        {\r\n          title: \"User Stories Document 1\",\r\n          iframes: [\"/doc/EW/UserStories1.pdf\"],\r\n          buttons: [\r\n            \"Pagina 1\",\r\n            \"Pagina 2\",\r\n            \"Pagina 3\",\r\n          ],\r\n          images: [\r\n            \"/images/UserStory1-1.png\",\r\n            \"/images/UserStory1-2.png\",\r\n            \"/images/UserStory1-3.png\",\r\n          ],\r\n        },\r\n        {\r\n          title: \"User Stories Document 2\",\r\n          iframes: [\"/doc/EW/UserStories2.pdf\"],\r\n          buttons: [\r\n            \"Pagina 1\",\r\n            \"Pagina 2\",\r\n          ],\r\n          images: [\r\n            \"/images/UserStory2-1.png\",\r\n            \"/images/UserStory2-2.png\",\r\n          ],\r\n        },\r\n        {\r\n          title: \"User Stories Document 3\",\r\n          iframes: [\"/doc/EW/UserStories3.pdf\"],\r\n          buttons: [\r\n            \"Pagina 1\",\r\n            \"Pagina 2\",\r\n            \"Pagina 3\",\r\n            \"Pagina 4\",\r\n          ],\r\n          images: [\r\n            \"/images/UserStory3-1.png\",\r\n            \"/images/UserStory3-2.png\",\r\n            \"/images/UserStory3-3.png\",\r\n            \"/images/UserStory3-4.png\",\r\n          ],\r\n        },\r\n      ],\r\n      selectedImages: Array(3).fill(0), // 3 documenten, init op pagina 0\r\n    };\r\n  },\r\n  methods: {\r\n    updateSelectedImage(docIndex, imageIndex) {\r\n      this.selectedImages[docIndex] = imageIndex;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Zelfde styling als jouw voorbeeld, eventueel aanpassen */\r\n.page-W2 {\r\n  color: white;\r\n  padding: 40px 20px;\r\n  max-width: 1400px;\r\n  margin: 0 auto;\r\n  font-family: sans-serif;\r\n}\r\n\r\n/* ...de rest van je styles kun je hergebruiken of aanpassen... */\r\n</style>\r\n"],"mappings":"AA0DA,eAAe;EACbA,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,CACT;QACEC,KAAK,EAAE,yBAAyB;QAChCC,OAAO,EAAE,CAAC,0BAA0B,CAAC;QACrCC,OAAO,EAAE,CACP,UAAU,EACV,UAAU,EACV,UAAU,CACX;QACDC,MAAM,EAAE,CACN,0BAA0B,EAC1B,0BAA0B,EAC1B,0BAA0B;MAE9B,CAAC,EACD;QACEH,KAAK,EAAE,yBAAyB;QAChCC,OAAO,EAAE,CAAC,0BAA0B,CAAC;QACrCC,OAAO,EAAE,CACP,UAAU,EACV,UAAU,CACX;QACDC,MAAM,EAAE,CACN,0BAA0B,EAC1B,0BAA0B;MAE9B,CAAC,EACD;QACEH,KAAK,EAAE,yBAAyB;QAChCC,OAAO,EAAE,CAAC,0BAA0B,CAAC;QACrCC,OAAO,EAAE,CACP,UAAU,EACV,UAAU,EACV,UAAU,EACV,UAAU,CACX;QACDC,MAAM,EAAE,CACN,0BAA0B,EAC1B,0BAA0B,EAC1B,0BAA0B,EAC1B,0BAA0B;MAE9B,CAAC,CACF;MACDC,cAAc,EAAEC,KAAK,CAAC,CAAC,CAAC,CAACC,IAAI,CAAC,CAAC,CAAC,CAAE;IACpC,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,mBAAmBA,CAACC,QAAQ,EAAEC,UAAU,EAAE;MACxC,IAAI,CAACN,cAAc,C