ass=\"image-wrapper\"\r\n        >\r\n          <img :src=\"imgSrc\" alt=\"Diagram\" />\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- ... jouw overige tekst hieronder ... -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive } from 'vue';\r\n\r\nexport default {\r\n  name: 'PageTwo',\r\n  setup() {\r\n    // Per document: array van 4 afbeeldingsgroepen (elk een array met img urls)\r\n    const images = [\r\n      [\r\n        ['/images/ActiviteitenDiagramTheKembitTimes.png'], // knop 1 voor doc 0\r\n        ['/images/ClassDiagramExample.png'],               // knop 2 voor doc 0\r\n        ['/images/ExtraImage1.png'],                        // knop 3 voor doc 0 (voorbeeld)\r\n        ['/images/ExtraImage2.png'],                        // knop 4 voor doc 0 (voorbeeld)\r\n      ],\r\n      [\r\n        ['/images/DataschemaERDExample.png'],              // knop 1 voor doc 1\r\n        ['/images/UseCaseDiagramExample.png'],             // knop 2 voor doc 1\r\n        ['/images/ExtraImage3.png'],                        // knop 3 voor doc 1 (voorbeeld)\r\n        ['/images/ExtraImage4.png'],                        // knop 4 voor doc 1 (voorbeeld)\r\n      ],\r\n      [\r\n        ['/images/MockupExample.png'],                      // knop 1 voor doc 2\r\n        ['/images/AnotherDiagram.png'],                     // knop 2 voor doc 2\r\n        ['/images/ExtraImage5.png'],                        // knop 3 voor doc 2 (voorbeeld)\r\n        ['/images/ExtraImage6.png'],                        // knop 4 voor doc 2 (voorbeeld)\r\n      ],\r\n    ];\r\n\r\n    const documents = [\r\n      '/doc/technischOntwerp_TheKEMBITTimes.pdf',\r\n      '/doc/technischOntwerp_TheKEMBITTimes.pdf',\r\n      '/doc/technischOntwerp_TheKEMBITTimes.pdf',\r\n    ];\r\n\r\n    // Houdt bij welke knop actief is per document (null = geen actief)\r\n    const activeButtons = reactive(Array(documents.length).fill(null));\r\n\r\n    function setActiveButton(docIndex, btnIndex) {\r\n      // Zet de actieve knop, klik op dezelfde knop toggle uit\r\n      if (activeButtons[docIndex] === btnIndex) {\r\n        activeButtons[docIndex] = null;\r\n      } else {\r\n        activeButtons[docIndex] = btnIndex;\r\n      }\r\n    }\r\n\r\n    return {\r\n      documents,\r\n      images,\r\n      activeButtons,\r\n      setActiveButton,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.documenten-blok-breed {\r\n  width: 100%;\r\n  padding: 40px 0;\r\n  background-color: #471f29;\r\n\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n  gap: 30px;\r\n  box-sizing: border-box;\r\n  overflow-x: auto;\r\n  align-items: flex-start;\r\n  position: relative;\r\n  flex-direction: column; /* belangrijk: iframe + buttons + images onder elkaar */\r\n}\r\n\r\n.documenten-blok-breed iframe {\r\n  flex: 1 1 450px;\r\n  max-width: 650px;\r\n  min-width: 450px;\r\n  height: 600px;\r\n  border-radius: 8px;\r\n  border: none;\r\n  margin: 0 auto;\r\n}\r\n\r\n/* Knoppen naast elkaar */\r\n.button-row {\r\n  display: flex;\r\n  gap: 10px;\r\n  margin: 20px auto 10px auto;\r\n  justify-content: center;\r\n  max-width: 650px;\r\n  width: 100%;\r\n}\r\n\r\n.button-row button {\r\n  flex: 1;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n  border: none;\r\n  background-color: #7a3f50;\r\n  color: #ffd6e0;\r\n  font-weight: bold;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n.button-row button:hover {\r\n  background-color: #90515f;\r\n}\r\n\r\n.active-button {\r\n  background-color: #ffd6e0;\r\n  color: #7a3f50;\r\n  font-weight: bolder;\r\n}\r\n\r\n/* Afbeeldingen container */\r\n.images-container {\r\n  max-width: 650px;\r\n  margin: 0 auto 40px auto;\r\n  display: flex;\r\n  gap: 15px;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n}\r\n\r\n.image-wrapper img {\r\n  max-width: 150px;\r\n  border-radius: 5px;\r\n  box-shadow: 0 0 8px rgba(255, 202, 212, 0.7);\r\n}\r\n</style>\r\n"],"mappings":