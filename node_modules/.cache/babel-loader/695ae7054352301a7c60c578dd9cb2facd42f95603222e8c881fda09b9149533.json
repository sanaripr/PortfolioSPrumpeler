oc7-img3.png\",\r\n            \"/images/doc7-img4.png\",\r\n          ],\r\n        },\r\n        {\r\n          title: \"Document 8\",\r\n          iframes: [\"/doc/doc8.pdf\"],\r\n          buttons: [\r\n            \"Structuur Diagram\",\r\n            \"Flowchart\",\r\n            \"Database Schema\",\r\n            \"Componenten Overzicht\",\r\n          ],\r\n          images: [\r\n            \"/images/doc8-img1.png\",\r\n            \"/images/doc8-img2.png\",\r\n            \"/images/doc8-img3.png\",\r\n            \"/images/doc8-img4.png\",\r\n          ],\r\n        },\r\n        {\r\n          title: \"Document 9\",\r\n          iframes: [\"/doc/doc9.pdf\"],\r\n          buttons: [\r\n            \"Use Case Overzicht\",\r\n            \"Class Schema\",\r\n            \"Activiteiten Overzicht\",\r\n            \"Systeem Diagram\",\r\n          ],\r\n          images: [\r\n            \"/images/doc9-img1.png\",\r\n            \"/images/doc9-img2.png\",\r\n            \"/images/doc9-img3.png\",\r\n            \"/images/doc9-img4.png\",\r\n          ],\r\n        },\r\n      ],\r\n\r\n      selectedImages: Array(9).fill(0),\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.page-W2 {\r\n  color: white;\r\n  padding: 40px;\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  font-family: sans-serif;\r\n}\r\n\r\nh1 {\r\n  margin-bottom: 20px;\r\n  color: #ffcad4;\r\n}\r\n\r\nh2,\r\nh3 {\r\n  margin-top: 20px;\r\n  color: #ffd6e0;\r\n}\r\n\r\n.text-block {\r\n  margin-bottom: 30px;\r\n  line-height: 1.6;\r\n}\r\n\r\n/* Column met alle documenten onder elkaar */\r\n.documents-column {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 50px;\r\n}\r\n\r\n/* Per document container: iframe + buttons + afbeelding */\r\n.document-container {\r\n  display: flex;\r\n  gap: 20px;\r\n  padding: 15px 0;\r\n  border-radius: 10px;\r\n  min-width: 320px;\r\n  align-items: flex-start;\r\n  /* geen achtergrondkleur */\r\n}\r\n\r\n/* iframe blok */\r\n.iframes-block {\r\n  flex: 2;\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 12px;\r\n}\r\n\r\n/* rechter kolom met buttons boven afbeelding */\r\n.side-block {\r\n  flex: 1;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: flex-start;\r\n  gap: 15px;\r\n  width: 100%;\r\n}\r\n\r\n/* Buttons naast elkaar in 1 rij */\r\n.buttons {\r\n  display: flex;\r\n  gap: 10px;\r\n  flex-wrap: nowrap;\r\n  justify-content: flex-start;\r\n  width: 100%;\r\n}\r\n\r\n.buttons button {\r\n  flex: 1 1 auto;\r\n  padding: 10px 14px;\r\n  font-size: 14px;\r\n  background-color: #444;\r\n  border: none;\r\n  color: white;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n  text-align: center;\r\n}\r\n\r\n.buttons button:hover {\r\n  background-color: #666;\r\n}\r\n\r\n.buttons button.active {\r\n  background-color: #ffcad4;\r\n  color: black;\r\n  font-weight: bold;\r\n}\r\n\r\n/* Afbeelding onder de buttons */\r\n.side-block img {\r\n  max-width: 100%;\r\n  max-height: 300px;\r\n  border-radius: 8px;\r\n  object-fit: contain;\r\n  box-shadow: 0 0 15px #aaa;\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAS;;EAkBbA,KAAK,EAAC;AAAkB;;EAMpBA,KAAK,EAAC;AAAe;mBAzBlC;;EAqCaA,KAAK,EAAC;AAAY;;EAChBA,KAAK,EAAC;AAAS;mBAtC9B;mBAAA;;uBACEC,mBAAA,CAyDM,OAzDNC,UAyDM,G,0BAxDJC,mBAAA,CAAoD,YAAhD,6CAA2C,sB,0BAE/CA,mBAAA,CAaM;IAbDH,KAAK,EAAC;EAAY,IACrBG,mBAAA,CAAgD,YAA5C,yCAAuC,GAC3CA,mBAAA,CAGI,WAHD,0OAGH,GACAA,mBAAA,CAGI,WAHD,+TAGH,GACAA,mBAAA,CAEI,WAFD,sJAEH,E,sBAGFA,mBAAA,CAoCM,OApCNC,UAoCM,I,kBAnCJH,mBAAA,CAkCMI,SAAA,QAtDZC,WAAA,CAqB+BC,KAAA,CAAAC,SAAS,EArBxC,CAqBgBC,GAAG,EAAEC,KAAK;yBADpBT,mBAAA,CAkCM;MAhCHU,GAAG,EAAED,KAAK;MACXV,KAAK,EAAC;QAENG,mBAAA,CAUM,OAVNS,UAUM,GATJT,mBAAA,CAAwB,YAAAU,gBAAA,CAAjBJ,GAAG,CAACK,KAAK,mB,kBAChBb,mBAAA,CAOUI,SAAA,QAlCpBC,WAAA,CA4B+BG,GAAG,CAACM,OAAO,EA5B1C,CA4BoBC,GAAG,EAAEC,CAAC;2BADhBhB,mBAAA,CAOU;QALPU,GAAG,EAAEM,CAAC;QACND,GAAG,EAAEA,GAAG;QACTE,KAAK,EAAC,MAAM;QACZC,