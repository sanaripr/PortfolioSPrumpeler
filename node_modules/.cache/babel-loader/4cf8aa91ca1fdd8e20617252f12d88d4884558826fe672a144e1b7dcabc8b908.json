edImage=\"selectedImages[index + 3]\"\r\n      />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"PageTwo\",\r\n  components: {\r\n    DocumentItem: {\r\n      props: [\"doc\", \"index\", \"modelValue\"],\r\n      emits: [\"update:modelValue\"],\r\n      template: `\r\n        <div class=\"document-container\">\r\n          <div class=\"iframes-block\">\r\n            <h3>{{ doc.title }}</h3>\r\n            <iframe\r\n              v-for=\"(src, i) in doc.iframes\"\r\n              :key=\"i\"\r\n              :src=\"src\"\r\n              width=\"100%\"\r\n              height=\"400px\"\r\n              frameborder=\"0\"\r\n            ></iframe>\r\n          </div>\r\n          <div class=\"side-block\">\r\n            <div class=\"buttons\">\r\n              <button\r\n                v-for=\"(btn, i) in doc.buttons\"\r\n                :key=\"i\"\r\n                @click=\"$emit('update:modelValue', i)\"\r\n                :class=\"{ active: modelValue === i }\"\r\n              >\r\n                {{ btn }}\r\n              </button>\r\n            </div>\r\n            <img :src=\"doc.images[modelValue]\" alt=\"Diagram\" />\r\n          </div>\r\n        </div>\r\n      `,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      documents: [\r\n        /* ... je 9 documenten hier ... */\r\n      ],\r\n      selectedImages: Array(9).fill(0),\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* ... je eerdere styles ... */\r\n\r\n.extra-text {\r\n  margin: 40px 0;\r\n  padding: 20px;\r\n  background-color: #333;\r\n  border-radius: 8px;\r\n  font-style: italic;\r\n  color: #eee;\r\n  max-width: 1200px;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n}\r\n</style>\r\n\r\n\r\n<style scoped>\r\n.page-W2 {\r\n  color: white;\r\n  padding: 40px;\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  font-family: sans-serif;\r\n}\r\n\r\nh1 {\r\n  margin-bottom: 20px;\r\n  color: #ffcad4;\r\n}\r\n\r\nh2,\r\nh3 {\r\n  margin-top: 20px;\r\n  color: #ffd6e0;\r\n}\r\n\r\n.text-block {\r\n  margin-bottom: 30px;\r\n  line-height: 1.6;\r\n}\r\n\r\n/* Column met alle documenten onder elkaar */\r\n.documents-column {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 50px;\r\n}\r\n\r\n/* Per document container: iframe links, buttons + afbeelding rechts */\r\n.document-container {\r\n  display: flex;\r\n  gap: 20px;\r\n  background: #222;\r\n  padding: 15px;\r\n  border-radius: 10px;\r\n  min-width: 320px;\r\n}\r\n\r\n/* iframe blok */\r\n.iframes-block {\r\n  flex: 2;\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 12px;\r\n}\r\n\r\n/* rechter kolom met buttons en afbeelding */\r\n.side-block {\r\n  flex: 1;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: flex-end;\r\n  gap: 12px;\r\n}\r\n\r\n/* Buttons grid: 2 per rij */\r\n.buttons {\r\n  display: grid;\r\n  grid-template-columns: repeat(2, 1fr);\r\n  gap: 10px 15px;\r\n  width: 100%;\r\n  max-width: 200px;\r\n}\r\n\r\n.buttons button {\r\n  padding: 8px 14px;\r\n  font-size: 14px;\r\n  background-color: #444;\r\n  border: none;\r\n  color: white;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n  width: 100%;\r\n}\r\n\r\n.buttons button:hover {\r\n  background-color: #666;\r\n}\r\n\r\n.buttons button.active {\r\n  background-color: #ffcad4;\r\n  color: black;\r\n  font-weight: bold;\r\n}\r\n\r\n/* Afbeelding onder buttons */\r\n.side-block img {\r\n  width: 100%;\r\n  border-radius: 8px;\r\n  object-fit: contain;\r\n  max-height: 200px;\r\n  box-shadow: 0 0 12px #aaa;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAS;;EASbA,KAAK,EAAC;AAAkB;;EAgBxBA,KAAK,EAAC;AAAkB;;;uBAzB/BC,mBAAA,CAkCM,OAlCNC,UAkCM,G,0BAjCJC,mBAAA,CAAoD,YAAhD,6CAA2C,sB,0BAE/CA,mBAAA,CAGM;IAHDH,KAAK,EAAC;EAAY,IACrBG,mBAAA,CAAgD,YAA5C,yCAAuC,GAC3CC,mBAAA,qBAAwB,C,sBAG1BA,mBAAA,yBAA4B,EAC5BD,mBAAA,CAQM,OARNE,UAQM,I,kBAPJJ,mBAAA,CAMEK,SAAA,QAjBRC,WAAA,CAY+BC,KAAA,CAAAC,SAAS,CAAC