class=\"{ active: selectedImages[index] === i }\"\r\n            >\r\n              {{ btn }}\r\n            </button>\r\n          </div>\r\n\r\n          <img\r\n            :src=\"doc.images[selectedImages[index]]\"\r\n            alt=\"Geselecteerde afbeelding\"\r\n          />\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <br /><br /><br />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"PageTwo\",\r\n  data() {\r\n    return {\r\n      documents: [\r\n        {\r\n          title: \"Document 1\",\r\n          iframes: [\"/doc/TechnischOntwerp_TheKembitTimes.pdf\"],\r\n          buttons: [\r\n            \"Schema Overzicht\",\r\n            \"Architectuur Diagram\",\r\n            \"Process Flow\",\r\n            \"Data Model\",\r\n          ],\r\n          images: [\r\n            \"/images/ActiviteitenDiagramTheKembitTimes.png\",\r\n            \"/images/ClassDiagramTheKEMBITTimes.png\",\r\n            \"/images/TheKembitTimes.png\",\r\n            \"/images/ActiviteitenDiagramTheKembitTimes.png\",\r\n          ],\r\n        },\r\n        {\r\n          title: \"Document 2\",\r\n          iframes: [\"/doc/doc2.pdf\"],\r\n          buttons: [\r\n            \"Use Case Diagram\",\r\n            \"Class Diagram\",\r\n            \"Sequence Diagram\",\r\n            \"State Diagram\",\r\n          ],\r\n          images: [\r\n            \"/images/doc2-img1.png\",\r\n            \"/images/doc2-img2.png\",\r\n            \"/images/doc2-img3.png\",\r\n            \"/images/doc2-img4.png\",\r\n          ],\r\n        },\r\n        {\r\n          title: \"Document 3\",\r\n          iframes: [\"/doc/doc3.pdf\"],\r\n          buttons: [\r\n            \"Context Diagram\",\r\n            \"Deployment Diagram\",\r\n            \"Activity Diagram\",\r\n            \"ER Diagram\",\r\n          ],\r\n          images: [\r\n            \"/images/doc3-img1.png\",\r\n            \"/images/doc3-img2.png\",\r\n            \"/images/doc3-i