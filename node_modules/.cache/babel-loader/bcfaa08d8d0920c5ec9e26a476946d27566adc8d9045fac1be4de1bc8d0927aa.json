r\n  padding: 15px 0;\r\n  border-radius: 10px;\r\n  min-width: 320px;\r\n  align-items: flex-start;\r\n  /* geen achtergrondkleur */\r\n}\r\n\r\n/* iframe blok */\r\n.iframes-block {\r\n  flex: 2;\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 12px;\r\n}\r\n\r\n/* rechter kolom met buttons boven afbeelding */\r\n.side-block {\r\n  flex: 1;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: flex-start;\r\n  gap: 15px;\r\n  width: 100%;\r\n}\r\n\r\n/* Buttons naast elkaar in 1 rij */\r\n.buttons {\r\n  display: flex;\r\n  gap: 10px;\r\n  flex-wrap: nowrap;\r\n  justify-content: flex-start;\r\n  width: 100%;\r\n}\r\n\r\n.buttons button {\r\n  flex: 1 1 auto;\r\n  padding: 10px 14px;\r\n  font-size: 14px;\r\n  background-color: #444;\r\n  border: none;\r\n  color: white;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n  text-align: center;\r\n}\r\n\r\n.buttons button:hover {\r\n  background-color: #666;\r\n}\r\n\r\n.buttons button.active {\r\n  background-color: #ffcad4;\r\n  color: black;\r\n  font-weight: bold;\r\n}\r\n\r\n/* Afbeelding onder de buttons */\r\n.side-block img {\r\n  max-width: 100%;\r\n  max-height: 300px;\r\n  border-radius: 8px;\r\n  object-fit: contain;\r\n  box-shadow: 0 0 15px #aaa;\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AA+FA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAEC,KAAK,CAAC,CAAC,CAAC,CAACC,IAAI,CAAC,CAAC,CAAC;MAChCC,SAAS,EAAE;MACT;MACA;QACEC,KAAK,EAAE,kBAAkB;QACzBC,OAAO,EAAE,CACP,yEAAwE,CACzE;QACDC,OAAO,EAAE,CAAC,cAAc,EAAE,cAAc,EAAE,cAAc,EAAE,cAAc,CAAC;QACzEC,MAAM,EAAE,CACN,uBAAuB,EACvB,uBAAuB,EACvB,uBAAuB,EACvB,uBAAsB;MAE1B,CAAC;MACD;MACA;QACEH,KAAK,EAAE,kBAAkB;QACzBC,OAAO,EAAE,CACP,yEAAwE,CACzE;QACDC,OAAO,EAAE,CAAC,cAAc,EAAE,cAAc,EAAE,cAAc,EAAE,cAAc,CAAC;QACzEC,MAAM,EAAE,CACN,uBAAuB,EACvB,uBAAuB,EACvB,uBAAuB,EACvB,uBAAsB;MAE1B,CAAC;MACD;MACA;QACEH,KAAK,EAAE,kBAAkB;QACzBC,OAAO,EAAE,CACP,yEAAwE,CACzE;QACDC,OAAO,EAAE,CAAC,cAAc,EAAE,cAAc,EA