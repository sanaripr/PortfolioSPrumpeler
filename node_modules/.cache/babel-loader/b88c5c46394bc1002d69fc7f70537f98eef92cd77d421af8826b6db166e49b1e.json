xample.png'],             \r\n      ],\r\n      [\r\n        ['/images/ActiviteitenDiagramTheKembitTimes.png'], \r\n        ['/images/ClassDiagramExample.png'],               \r\n        ['/images/DataschemaERDExample.png'],              \r\n        ['/images/UseCaseDiagramExample.png'],             \r\n      ],\r\n    ];\r\n\r\n    // Actieve knop per document (null = geen actief)\r\n    const activeButtons = reactive(Array(documents.length).fill(null));\r\n\r\n    // Functie om active knop te zetten of uit te zetten (toggle)\r\n    function setActiveButton(docIndex, btnIndex) {\r\n      if (activeButtons[docIndex] === btnIndex) {\r\n        activeButtons[docIndex] = null; // toggle uitzetten\r\n      } else {\r\n        activeButtons[docIndex] = btnIndex;\r\n      }\r\n    }\r\n\r\n    return {\r\n      documents,\r\n      buttonLabels,\r\n      images,\r\n      activeButtons,\r\n      setActiveButton,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.page-wrapper {\r\n  color: white;\r\n  font-family: Arial, sans-serif;\r\n  padding: 20px;\r\n  background-color: #2c1a22;\r\n  min-height: 100vh;\r\n}\r\n\r\n.documenten-blok-breed {\r\n  width: 100%;\r\n  padding: 40px 0;\r\n  background-color: #471f29;\r\n\r\n  display: flex;\r\n  flex-direction: column; /* iframe + buttons + images onder elkaar */\r\n  align-items: center;\r\n  gap: 20px;\r\n  box-sizing: border-box;\r\n  border-radius: 8px;\r\n  margin-bottom: 50px;\r\n  overflow-x: auto;\r\n}\r\n\r\n.documenten-blok-breed iframe {\r\n  width: 650px;\r\n  height: 600px;\r\n  border-radius: 8px;\r\n  border: none;\r\n}\r\n\r\n/* Knoppen naast elkaar */\r\n.button-row {\r\n  display: flex;\r\n  gap: 10px;\r\n  justify-content: center;\r\n  width: 650px;\r\n}\r\n\r\n.button-row button {\r\n  flex: 1;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n  border: none;\r\n  background-color: #7a3f50;\r\n  color: #ffd6e0;\r\n  font-weight: bold;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s;\r\n  white-space: nowrap;\r\n  font-size: 14px;\r\n}\r\n\r\n.button-row button:hover {\r\n  background-color: #90515f;\r\n}\r\n\r\n.active-button {\r\n  background-color: #ffd6e0;\r\n  color: #7a3f50;\r\n  font-weight: bolder;\r\n}\r\n\r\n/* Afbeeldingen container */\r\n.images-container {\r\n  width: 650px;\r\n  display: flex;\r\n  gap: 15px;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n}\r\n\r\n.image-wrapper img {\r\n  max-width: 150px;\r\n  border-radius: 5px;\r\n  box-shadow: 0 0 8px rgba(255, 202, 212, 0.7);\r\n}\r\n\r\n/* Footer */\r\n.page-wrapper p:last-child {\r\n  margin-top: 50px;\r\n  font-size: 12px;\r\n  text-align: center;\r\n  color: #ccc;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAc;mBAD3B;;EAeWA,KAAK,EAAC;AAAY;mBAf7B;;EAAAC,GAAA;EA2B0CD,KAAK,EAAC;;mBA3BhD;;uBACEE,mBAAA,CAsCM,OAtCNC,UAsCM,G,0BArCJC,mBAAA,CAAoD,WAAjD,+CAA6C,sB,0BAChDA,mBAAA,CAAmD,WAAhD,8CAA4C,sB,0BAC/CA,mBAAA,CAAyB,WAAtB,oBAAkB,sBAErBC,mBAAA,sBAAyB,G,kBACzBH,mBAAA,CA6BMI,SAAA,QApCVC,WAAA,CASgCC,MAAA,CAAAC,SAAS,EATzC,CAScC,GAAG,EAAEC,QAAQ;yBAFvBT,mBAAA,CA6BM;MA5BJF,KAAK,EAAC,uBAAuB;MAE5BC,GAAG,EAAEU;QAENP,mBAAA,CAA4C;MAAnCQ,GAAG,EAAEF,GAAG;MAAEG,WAAW,EAAC;4BAZrCC,UAAA,GAcMT,mBAAA,4BAA+B,EAC/BD,mBAAA,CASM,OATNW,UASM,I,cARJb,mBAAA,CAOSI,SAAA,QAvBjBC,WAAA,CAiB6B,CAAC,EAAbS,QAAQ;aADjBZ,mBAAA,CAOS;QALNH,GAAG,EAAEe,QAAQ;QACbhB,KAAK,EAnBhBiB,eAAA;UAAA,iBAmBoCT,MAAA,CAAAU,aAAa,CAACP,QAAQ,MAAMK;QAAQ;QAC7DG,OAAK,EAAAC,MAAA,IAAEZ,MAAA,CAAAa,eAAe,CAACV,QAAQ,EAAEK,QAAQ;0BAEvCR,MAAA,CAAAc,YAAY,CAACN,QAAQ,qCAtBlCO,UAAA;sCA0BMlB,mBAAA,yCAA4C,EACjCG,MAAA,CAAAU,aAAa,CAACP,QAAQ,K,cAAjCT,mBAAA,CAQM,OARNsB,UAQM,I,kBAPJtB,mBAAA,CAMMI,SAAA,QAlCdC,WAAA,CA6BuCC,MAAA,CAAAiB,MAAM,CAACd,QAAQ,EAAEH,MAAA,CAAAU,aAAa,CAACP,QAAQ,QA7B9E,CA6BkBe,MAAM,EAAEC,QAAQ;2BAD1BzB,mBAAA,CAMM;QAJHD,GAAG,EAAE0B,QAAQ;QACd3B,KAAK,EAAC;UAENI,mBAAA,CAAmC;QAA7BQ,GAAG,EAAEc,MAAM;QAAEE,GAAG,EAAC;8BAjCjCC,UAAA,E;wCAAAxB,mBAAA,e;4DAsCID,mBAAA,CAA+D,WAA5D,0DAAwD,qB","ignoreList":[]},"metadata":{},"sourceType":"module","