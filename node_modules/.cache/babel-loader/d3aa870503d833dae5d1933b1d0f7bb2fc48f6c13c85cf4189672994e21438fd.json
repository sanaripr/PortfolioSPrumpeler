       v-for=\"(img, i) in doc.images\"\r\n          :key=\"i\"\r\n          :src=\"img\"\r\n          alt=\"Document preview\"\r\n          class=\"preview-image\"\r\n        />\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Documents\",\r\n  data() {\r\n    return {\r\n      documents: [\r\n        {\r\n          title: \"Eisen en wensen in user stories\",\r\n          iframes: [\r\n            \"/doc/userstories/document1.pdf\",\r\n            \"/doc/userstories/document2.pdf\",\r\n            \"/doc/userstories/document3.pdf\",\r\n          ],\r\n          buttons: [\"Document 1\", \"Document 2\", \"Document 3\"],\r\n          images: [\r\n            \"/images/userstories1.png\",\r\n            \"/images/userstories2.png\",\r\n            \"/images/userstories3.png\",\r\n          ],\r\n        },\r\n        {\r\n          title: \"Projectplan\",\r\n          iframes: [\"/doc/projectplan/projectplan.pdf\"],\r\n          buttons: [\"Projectplan PDF\"],\r\n          images: [\"/images/projectplan.png\"],\r\n        },\r\n        {\r\n          title: \"Ontwerpdocumentatie\",\r\n          iframes: [\"/doc/ontwerp/ontwerp.pdf\"],\r\n          buttons: [\"Ontwerp PDF\"],\r\n          images: [\"/images/ontwerp.png\"],\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    onButtonClick(docTitle, btnName) {\r\n      alert(`Button \"${btnName}\" clicked in document: ${docTitle}`);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.documents-wrapper {\r\n  max-width: 1200px;\r\n  margin: 2rem auto;\r\n  padding: 0 1rem;\r\n  font-family: Arial, sans-serif;\r\n  color: #222;\r\n}\r\n\r\n.document-section {\r\n  border-bottom: 1px solid #ccc;\r\n  padding-bottom: 2rem;\r\n  margin-bottom: 2rem;\r\n}\r\n\r\n.document-title {\r\n  font-size: 1.8rem;\r\n  margin-bottom: 1rem;\r\n  color: #333;\r\n}\r\n\r\n/* Container for iframes */\r\n.iframe-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  flex-wrap: wrap;\r\n  gap: 1rem;\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n/* When multiple iframes (like user stories), set width smaller */\r\n.multiple-iframes .doc-iframe {\r\n  width: 300px;\r\n  height: 400px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n}\r\n\r\n/* For single iframe, make it wider */\r\n.iframe-container:not(.multiple-iframes) .doc-iframe {\r\n  width: 90%;\r\n  max-width: 800px;\r\n  height: 600px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 6px;\r\n}\r\n\r\n.buttons {\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.buttons button {\r\n  margin-right: 0.75rem;\r\n  padding: 0.4rem 1rem;\r\n  font-size: 1rem;\r\n  cursor: pointer;\r\n  background-color: #2d8cf0;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 3px;\r\n  transition: background-color 0.2s ease-in-out;\r\n}\r\n\r\n.buttons button:hover {\r\n  background-color: #1a6ed8;\r\n}\r\n\r\n.images {\r\n  display: flex;\r\n  gap: 1rem;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.preview-image {\r\n  width: 120px;\r\n  height: auto;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  object-fit: contain;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAmB;;EAQtBA,KAAK,EAAC;AAAgB;mBAThC;;EAyBWA,KAAK,EAAC;AAAS;mBAzB1B;;EAmCWA,KAAK,EAAC;AAAQ;mBAnCzB;;uBACEC,mBAAA,CA4CM,OA5CNC,UA4CM,G,0BA3CJC,mBAAA,CAA6B,YAAzB,sBAAoB,uB,kBAExBF,mBAAA,CAwCUG,SAAA,QA5CdC,WAAA,CAK6BC,KAAA,CAAAC,SAAS,EALtC,CAKcC,GAAG,EAAEC,KAAK;yBADpBR,mBAAA,CAwCU;MAtCPS,GAAG,EAAED,KAAK;MACXT,KAAK,EAAC;QAENG,mBAAA,CAA+C,MAA/CQ,UAA+C,EAAAC,gBAAA,CAAjBJ,GAAG,CAACK,KAAK,kBAEvCV,mBAAA,CAYM;MAXJH,KAAK,EAZbc,eAAA,EAYc,kBAAkB;QAAA,oBACKN,GAAG,CAACO,OAAO,CAACC,MAAM;MAAA;2BAE/Cf,mBAAA,CAOUG,SAAA,QAtBlBC,WAAA,CAgB6BG,GAAG,CAACO,OAAO,EAhBxC,CAgBkBE,GAAG,EAAEC,CAAC;2BADhBjB,mBAAA,CAOU;QALPS,GAAG,EAAEQ,CAAC;QACND,GAAG,EAAEA,GAAG;QACTE,WAAW,EAAC,GAAG;QACfC,eAAe,EAAf,EAAe;QACfpB,KAAK,EAAC;8BArBhBqB,UAAA;qDAyBMlB,mBAAA,CAQM,OARNmB,UAQM,I,kBAPJrB,mBAAA,CAMSG,SAAA,QAhCjBC,WAAA,CA2B6BG,GAAG,CAACe,OAAO,EA3BxC,C